import chords
import tablature.grammar

Program:
	header=SongHeader
	sequences*=SongSequence
;

SongHeader:
"["
	"Name" name=STRING
	"Author" author=STRING
	"Beat" beatup=INT "/" beatdown=INT
	"Tempo" tempo=INT
"]"
;

SongSequence:
	"sequence" type=InstrumentType name=STRING 
	"{"
	
	value=SourceType 
	
	"}"
;

InstrumentType:
	"guitar-solo"|"guitar-rhythm"|"drums"|"keyboards"|"bass"|"electro"
;

SourceType:
	GuitarChords|..tablature.grammar.Tablature
;

Comment:
  /\/\/.*$/
;